{
    "name": "wordtable",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "gulp": "node_modules/.bin/gulp",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "npm run gulp && nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "NODE_ENV=deployment yarn build && NODE_ENV=deployment node dist/src/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "e2e": "NODE_ENV=e2e npm run gulp && NODE_ENV=e2e jest --config ./jest-e2e.config.json --verbose"
    },
    "dependencies": {
        "@daihere1993/dsp": "^0.0.2",
        "@nestjs/common": "^6.10.14",
        "@nestjs/core": "^6.10.14",
        "@nestjs/jwt": "^6.1.2",
        "@nestjs/passport": "^6.2.0",
        "@nestjs/platform-express": "^6.10.14",
        "bcrypt": "^4.0.0",
        "cheerio": "^1.0.0-rc.3",
        "config": "^3.2.6",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.3",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.4",
        "underscore": "^1.9.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^6.13.2",
        "@nestjs/schematics": "^6.8.1",
        "@nestjs/testing": "^6.10.14",
        "@types/bcrypt": "^3.0.0",
        "@types/cheerio": "^0.22.16",
        "@types/config": "^0.0.36",
        "@types/express": "^4.17.2",
        "@types/jest": "25.1.2",
        "@types/jsonwebtoken": "^8.3.7",
        "@types/mongoose": "^5.7.3",
        "@types/node": "^13.1.6",
        "@types/supertest": "^2.0.8",
        "@types/temp-write": "^4.0.0",
        "@types/underscore": "^1.9.4",
        "@types/winston": "^2.4.4",
        "@types/xml-js": "^1.0.0",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "^2.19.1",
        "gulp": "^4.0.2",
        "gulp-rename": "^2.0.0",
        "gulp-util": "^3.0.8",
        "gulp-xml2json": "^0.1.5",
        "jest": "^25.1.0",
        "map-stream": "^0.0.7",
        "prettier": "^1.18.2",
        "supertest": "^4.0.2",
        "temp-write": "^4.0.0",
        "ts-jest": "25.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.0",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^6.0.0",
        "tslint-config-alloy": "^0.2.1",
        "typescript": "^3.7.4",
        "xml-js": "^1.6.11"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "roots": [
            "./test/ut"
        ],
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^@app/(.*)$": "<rootDir>/src/$1",
            "^@test/(.*)$": "<rootDir>/test/$1"
        }
    },
    "main": "index.js"
}
